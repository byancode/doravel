
# ENVIRONMETS
APP_ENV=local

# FUNCTiONS
is_true() {
    case "$1" in
        true|True|TRUE|1) return 0 ;;
        *) return 1 ;;
    esac
}

is_false() {
    case "$1" in
        false|False|FALSE|0) return 0 ;;
        *) return 1 ;;
    esac
}

# EXPORTS
if [ -d /root/.local/share/fnm/aliases/default/bin ]; then
    export PATH="/root/.local/share/fnm/aliases/default/bin:$PATH"
fi

# ALIASES
alias d="doravel"